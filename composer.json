{
	"name": "cubitworx/laravel-languages",
	"version": "1.0.1",
	"description": "Table of langauges as per ISO 639-1 & ISO 639-3",
	"keywords": ["laravel", "languages", "ISO 639-1", "ISO 639-3"],
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Charles Louw",
			"email": "charles.louw@gmail.com"
		}
	],
	"require": {
		"php": "^7.1.3",
		"laravel/framework": "^5.5.0"
	},
	"autoload": {
		"psr-4": {
			"Cubitworx\\Laravel\\Languages\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Cubitworx\\Laravel\\Languages\\Providers\\ServiceProvider"
			]
		}
	}
}
